function copyData(savPath, srcPath) {
	fs.readFile(srcPath, 'utf8', function (err, data) {
		if (err) throw err;
		// process input file
		fs.writeFile (savPath, data, function(err) {
			if (err) throw err;
			console.log('complete');
		});
	});
